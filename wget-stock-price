get today's GOOGL price
wget -O test.price 'http://finance.yahoo.com/d/quotes.csv?s=GOOGL&f=snohgl1v'

get GOOGL price on 2016-06-09
wget -O googl.log 'http://ichart.finance.yahoo.com/table.csv?s=GOOGL&a=05&b=09&c=2016&d=05&e=09&f=2016&g=d&ignore=.csv'

get GOOGL from 2016-06-01 to 2016-06-09
wget -O googl.log 'http://ichart.finance.yahoo.com/table.csv?s=GOOGL&a=05&b=01&c=2016&d=05&e=09&f=2016&g=d&ignore=.csv'

get GOOGL price from 2010-01-01 to today
wget -O googl.log 'http://ichart.finance.yahoo.com/table.csv?s=GOOGL&a=00&b=01&c=2010&g=d&ignore=.csv'

get GOOGL all price
wget -O googl.log 'http://ichart.finance.yahoo.com/table.csv?s=GOOGL&g=d&ignore=.csv'
